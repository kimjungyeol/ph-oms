<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ktnet.fta.item.mapper.ItemMapper">

    <select id="selectHsCodeList" parameterType="map" resultType="camelMap">
        /*queryId=SampleMapper.selectHsCodeList*/
        SELECT 
        	   DISTINCT HS_CODE 
        	  ,ITEM_NM 
          FROM CMPNY_ITEM
         WHERE HS_CODE IS NOT NULL
    </select>

</mapper>